    "@tanstack/react-table": "^8.10.3",
    "axios": "^1.6.0",
    "clsx": "^2.0.0",
    "eslint-config-standard-with-typescript": "^39.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-react": "^7.33.2",
    "prettier-eslint": "^15.0.1",
